{
  "name": "Student",
  "plural": "Students",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins":{
     "ClearBaseAcls": true
  },
  "properties": {
    "degree": {
      "type": "string",
      "required": true,
      "default": "bachelor"
    },
    "descriptions": {
      "type": "string"
    },
    "location": {
      "type": "geopoint"
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "tutor": {
      "type": "boolean",
      "required": true
    },
    "rating": {
      "type": "number",
      "required": true
    },
    "availability": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "securityQuestion": {
      "type": "string",
      "required": true
    },
    "securityAnswer": {
      "type": "string",
      "required": true
    },
    "university": {
      "type": "string"
    },
    "messageReceived":{
      "type":"string"
    },
    "messageSent":{
      "type":"string"     
    }
  },
  "validations": [],
  "relations": {
    "userPhotos": {
      "type": "hasMany",
      "model": "UserPhoto",
      "foreignKey": "userId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    }
  },
 "methods": {},
 "acls":[] 
}
